@model Vendor

@{
    Layout = "_Layout";
}

<!DOCTYPE html>

<html>
<head>
    <title>Vendor @Model.name</title>
</head>
<body>
<div>
     <table>
             <tbody>
             @foreach (var item in @Model.orders)
             {
                 <tr>
                     <td>@item.Title</td>
                     <td>@item.Price</td>
                 </tr>
             }
             </tbody>
         </table>
</div>
</body>
</html>