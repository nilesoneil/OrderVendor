@{
    Layout = "_Layout";
}
@model List<Vendor>
<!DOCTYPE html>
<div>
    <table>
        <tbody>
        @foreach (var item in @Model)
        {
            <tr>
                <td>@item.name</td>
                <td>@item.description</td>
                <td>@Html.ActionLink("Details", "Details", new {vendorId = item.id})</td>
            </tr>
        }
        </tbody>
    </table>
    <p>@Html.ActionLink("Add New Vendor", "Create")</p>
</div>